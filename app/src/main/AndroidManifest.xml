<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hackathon.cheherlock" >
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.write_external_storage"/>
    <uses-permission android:name="android.permission.read_external_storage"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_app_icon"
        android:label="@string/app_name"
        android:theme="@style/MyMaterialTheme" >

        <activity
            android:name="com.hackathon.cheherlock.activity.LoginActivity"
            android:label="@string/app_name" >

        </activity>
        <activity
            android:name="com.hackathon.cheherlock.activity.SignupActivity"
            android:label="@string/app_name" >

        </activity>
        <activity
            android:name="com.hackathon.cheherlock.activity.StartActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name="com.hackathon.cheherlock.activity.MainActivity"
            android:label="@string/app_name" >

        </activity>
        <activity
            android:name="com.hackathon.cheherlock.activity.SearchFilterActivity"
            android:label="@string/app_name" >

        </activity>



    </application>

</manifest>
